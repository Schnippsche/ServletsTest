    </servlet>
    <servlet>
        <servlet-name>importstatus</servlet-name>
        <servlet-class>de.destatis.regdb.servlets.RegDBImportstatusServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>select</servlet-name>
        <url-pattern>/select</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>singleselect</servlet-name>
        <url-pattern>/singleselect</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
