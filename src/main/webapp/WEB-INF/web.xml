    </servlet>
    <servlet>
        <servlet-name>dateiimport</servlet-name>
        <servlet-class>de.destatis.regdb.servlets.RegDBImportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download</servlet-name>
        <servlet-class>de.destatis.regdb.servlets.RegDBDownloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>session</servlet-name>
        <servlet-class>de.destatis.regdb.servlets.RegDBSessionServlet</servlet-class>
